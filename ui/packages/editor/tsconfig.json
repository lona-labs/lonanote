{
  "compilerOptions": {
    "baseUrl": "./src",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "emitDeclarationOnly": true,
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "jsxImportSource": "vue",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "rootDir": "src",
    "skipLibCheck": true,
    "target": "esnext"
  },
  "include": ["src"],
}
