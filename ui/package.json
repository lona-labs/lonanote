{
  "name": "lonanote",
  "version": "0.0.2",
  "description": "lonanote",
  "author": "lona-labs",
  "homepage": "https://github.com/lona-labs/lonanote",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "start": "pnpm --filter=\"./packages/desktop\" run start",
    "dev": "pnpm run build:rust:node && pnpm --filter=\"./packages/desktop\" run dev",
    "dev:web": "pnpm --filter=\"./packages/renderer\" run dev",
    "dev:android": "pnpm tauri:cmd android dev --open",
    "dev:ios": "pnpm tauri:cmd ios dev --open --host",
    "build": "pnpm run build:rust:node && pnpm --filter=\"./packages/desktop\" run build",
    "build:web": "pnpm --filter=\"./packages/desktop\" run build:web",
    "build:android": "pnpm tauri:cmd android build --open",
    "build:ios": "pnpm tauri:cmd ios build --open",
    "postinstall": "tsx ./scripts/postinstall.ts",
    "build:unpack": "pnpm run build:rust:node && pnpm --filter=\"./packages/desktop\" run build:unpack",
    "build:win": "pnpm run build:rust:node && pnpm --filter=\"./packages/desktop\" run build:win",
    "build:mac": "pnpm run build:rust:node && pnpm --filter=\"./packages/desktop\" run build:mac",
    "build:linux": "pnpm run build:rust:node && pnpm --filter=\"./packages/desktop\" run build:linux",
    "build:rust:node": "pnpm -C ../rust/node build",
    "build:rust:node:debug": "pnpm -C ../rust/node build:debug",
    "tauri:cmd": "env-cmd -f scripts/env/tauri.env -- tauri"
  },
  "devDependencies": {
    "@codemirror/autocomplete": "=6.18.6",
    "@codemirror/commands": "=6.1.2",
    "@codemirror/language": "=6.11.0",
    "@codemirror/language-data": "=6.5.1",
    "@codemirror/search": "=6.5.10",
    "@codemirror/state": "=6.5.2",
    "@codemirror/view": "=6.23.1",
    "@ctrl/tinycolor": "^4.1.0",
    "@fsegurai/codemirror-theme-bundle": "^6.1.2",
    "@milkdown/core": "^7.10.1",
    "@milkdown/crepe": "^7.10.1",
    "@milkdown/plugin-automd": "^7.10.1",
    "@milkdown/plugin-diagram": "^7.7.0",
    "@milkdown/plugin-emoji": "^7.10.1",
    "@milkdown/plugin-prism": "^7.10.1",
    "@milkdown/utils": "^7.10.1",
    "@tauri-apps/api": "^2.5.0",
    "@tauri-apps/cli": "^2.5.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/codemirror": "^5.60.15",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "chroma-js": "^3.1.2",
    "clsx": "^2.1.1",
    "codemirror": "^5.65.19",
    "colorjs.io": "^0.5.2",
    "dayjs": "^1.11.13",
    "env-cmd": "^10.1.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "framer-motion": "^12.7.4",
    "globals": "^16.0.0",
    "path-browserify-esm": "^1.0.6",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "sass-embedded": "^1.86.3",
    "ts-deepmerge": "^7.0.2",
    "tsup": "8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vconsole": "^3.15.1",
    "vite": "^6.3.2",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-externalize-deps": "^0.9.0",
    "vite-plugin-vconsole": "^2.1.1"
  },
  "resolutions": {
    "@codemirror/commands": "=6.1.2",
    "@codemirror/view": "=6.23.1"
  },
  "packageManager": "pnpm@9.15.0",
  "node": ">=20.0.0"
}
