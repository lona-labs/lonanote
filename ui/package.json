{
  "name": "lonanote",
  "version": "0.0.1",
  "description": "lonanote",
  "main": "./out/main/index.js",
  "author": "lona-labs",
  "homepage": "https://github.com/lona-labs/lonanote",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "pnpm run build:rust:node && electron-vite dev",
    "dev:web": "vite --force",
    "dev:android": "pnpm tauri:cmd android dev --open",
    "dev:ios": "pnpm tauri:cmd ios dev --open --host",
    "build": "pnpm run build:rust:node && pnpm run build:web",
    "build:web": "pnpm run typecheck && electron-vite build",
    "build:android": "pnpm tauri:cmd android build --open",
    "build:ios": "pnpm tauri:cmd ios build --open",
    "postinstall": "electron-builder install-app-deps && tsx ./scripts/postinstall.ts",
    "build:unpack": "pnpm run build && electron-builder --dir",
    "build:win": "pnpm run build && electron-builder --win",
    "build:mac": "pnpm run build && electron-builder --mac",
    "build:linux": "pnpm run build && electron-builder --linux",
    "build:rust:node": "pnpm -C ../rust/node build",
    "build:rust:node:debug": "pnpm -C ../rust/node build:debug",
    "tauri:cmd": "env-cmd -f scripts/env/tauri.env -- tauri"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.6.2",
    "lonanote-core-node": "link:../rust/node"
  },
  "devDependencies": {
    "@codemirror/autocomplete": "=6.18.6",
    "@codemirror/commands": "=6.1.2",
    "@codemirror/language": "=6.11.0",
    "@codemirror/language-data": "=6.5.1",
    "@codemirror/search": "=6.5.10",
    "@codemirror/state": "=6.5.2",
    "@codemirror/view": "=6.23.1",
    "@ctrl/tinycolor": "^4.1.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@radix-ui/themes": "^3.2.1",
    "@tauri-apps/api": "^2.3.0",
    "@tauri-apps/cli": "^2.3.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/codemirror": "^5.60.15",
    "@types/node": "^22.13.11",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "allotment": "^1.20.3",
    "chroma-js": "^3.1.2",
    "clsx": "^2.1.1",
    "codemirror": "^5.65.19",
    "colorjs.io": "^0.5.2",
    "dayjs": "^1.11.13",
    "electron": "^35.0.3",
    "electron-builder": "^26.0.12",
    "electron-builder-squirrel-windows": "^26.0.12",
    "electron-vite": "^3.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "framer-motion": "^12.5.0",
    "globals": "^16.0.0",
    "hypermd": "^0.3.11",
    "next-themes": "^0.4.6",
    "path-browserify-esm": "^1.0.6",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.4.0",
    "react-toastify": "^11.0.5",
    "react-virtuoso": "^4.12.5",
    "sass-embedded": "^1.86.0",
    "ts-deepmerge": "^7.0.2",
    "tsx": "^4.19.3",
    "turndown-plugin-gfm": "^1.0.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0",
    "vconsole": "^3.15.1",
    "vditor": "=3.10.8",
    "vite": "^6.2.2",
    "vite-plugin-vconsole": "^2.1.1",
    "zustand": "^5.0.3"
  },
  "resolutions": {
    "@codemirror/commands": "=6.1.2",
    "@codemirror/view": "=6.23.1",
    "codemirror": "^5.65.19"
  },
  "packageManager": "pnpm@9.15.0",
  "node": ">=20.0.0"
}
